      </header>
      <main class="wrappper__main">
        <div class="wrappper__sider">
          <div class="wrappper__sider_01">
            <!-- <p>Reference Lan:</p> -->
            <select>
              <% lans.forEach(function(lan){ %>
              <option value="<%= lan %>"><%= lan %></option>
              <% }); %>
            </select>
          </div>
          <div class="wrappper__sider_03">
            <!-- <p>Commit Ids:</p> -->
            <input id="commit-ids" class="ui-input" placeholder="OR, 多个 commit id 用 , 逗号分隔">
          </div>
          <div class="wrappper__sider_02">
            <p>Select Languages:</p>
            <div class="wrappper__sider_02-label">
              <% lans.forEach(function(lan, idx){ %>
              <div class="div-lan div-<%= lan %>">
                <input
                  type="checkbox"
                  id="checkbox<%=idx%>"
                  value="<%= lan %>"
                  name="checkbox"
                />
                <label for="checkbox<%=idx%>" class="ui-checkbox"></label
                ><label for="checkbox<%=idx%>" style="padding-left: 5px;"><%= lan %></label>
              </div>
              <% }); %>
            </div>
            <a
              href="javascript:"
              class="ui-button ui-button-primary handle-btn"
              role="button"
              >Operate</a
            >
          </div>
        </div>
        <div class="wrappper__content">
          <div class="wrappper__content-header"></div>
          <div class="wrappper__content-content"></div>
        </div>
        <!-- <div class="wrappper__sider wrappper__sider2">
        </div> -->
      </main>
