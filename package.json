{
  "name": "zdh",
  "version": "2.1.14",
  "private": true,
  "bin": "./bin/www",
  "scripts": {
    "dev": "nodemon node ./bin/www",
    "start": "node ./bin/www",
    "bump-version": "node bump-version.js",
    "build": "pkg . --out-path dist",
    "build:win": "pkg . --targets node18-win-x64 --out-path dist",
    "build:linux": "pkg . --targets node18-linux-x64 --out-path dist",
    "build:mac": "pkg . --targets node18-macos-x64 --out-path dist"
  },
  "browserslist": [
    "> 0%"
  ],
  "dependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "acorn": "^8.13.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.1.0",
    "chokidar": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "diff": "^7.0.0",
    "ejs": "^3.1.10",
    "eslint": "^9.13.0",
    "eslint-visitor-keys": "^4.2.0",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.7",
    "playwright": "^1.53.1",
    "postcss": "^8.4.49",
    "postcss-logical": "^8.0.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "rtlcss": "^4.3.0",
    "sass": "^1.79.4",
    "simple-git": "^3.27.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "ssh2-sftp-client": "^11.0.0",
    "stylelint": "^16.10.0",
    "stylelint-scss": "^6.8.1",
    "terser": "^5.34.0"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": [
      "bin/www",
      "app.js",
      "routes/**/*.js",
      "utils/**/*.js",
      "*.js"
    ],
    "assets": [
      "views/**/*",
      "public/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  }
}
