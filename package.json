{
  "name": "zdh",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "nodemon node ./bin/www",
    "start": "electron .",
    "build": "electron-builder --win portable"
  },
  "build": {
    "appId": "com.yourcompany.lanasync",
    "productName": "LanAsync",
    "win": {
      "target": "portable",
      "icon": "img/async.svg"
    },
    "files": [
      "**/*",
      "!**/*.map"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "E:\\zdh\\dist"
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "chokidar": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "puppeteer": "^23.4.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "ssh2-sftp-client": "^11.0.0",
    "terser": "^5.34.0"
  },
  "devDependencies": {
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "node-sass": "^9.0.0"
  }
}
