{
  "name": "zdh",
  "version": "2.0.27",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "nodemon node ./bin/www",
    "start": "electron .",
    "bump-version": "node bump-version.js",
    "build": "npm run bump-version && electron-builder --win portable",
    "dir": "electron-builder --dir"
  },
  "build": {
    "appId": "com.yourcompany.vidnozasync",
    "productName": "VidnozAsync",
    "win": {
      "target": "portable",
      "icon": "favicon.ico"
    },
    "files": [
      "**/*",
      "!**/*.map",
      "node_modules/@babel/preset-env/**/*"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "E:\\zdh\\dist"
    }
  },
  "browserslist": [
    "> 0%"
  ],
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "acorn": "^8.13.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "chokidar": "^3.5.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "diff": "^5.1.0",
    "ejs": "~2.6.1",
    "eslint": "^8.56.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.0.2",
    "playwright": "^1.40.1",
    "postcss": "^8.4.32",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "simple-git": "^3.21.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "ssh2-sftp-client": "^11.0.0",
    "stylelint": "^15.11.0",
    "stylelint-scss": "^5.3.1",
    "terser": "^5.26.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  }
}